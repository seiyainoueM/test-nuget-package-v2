{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "timezone": "Asia/Tokyo",
    "automerge": true,
    "packageRules": [
        {
            "matchUpdateTypes": [
                "major"
            ],
            "labels": [
                "Update-Major"
            ]
        },
        {
            "matchUpdateTypes": [
            "minor"
            ],
            "labels": [
            "Update-Minor"
            ]
        },
        {
            "matchUpdateTypes": [
            "patch"
            ],
            "labels": [
            "Update-Patch"
            ],
            "ignoreTests": true
        }
    ],
    "branchPrefix": "renovate/",
    "dryRun": false,
    "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
    "onboarding": false,
    "platform": "github",
    "includeForks": true,
    "dependencyDashboard": true,
    "repositories": [
        "seiyainoueM/test-nuget-package-v2"
    ]
}